.title {
    justify-content: flex-start;
    align-items: auto;
    -unity-text-align: middle-center;
    background-color: rgb(77, 77, 77);
    -unity-font-style: bold;
    border-top-width: 0;
    border-bottom-width: 2px;
    border-bottom-color: rgb(32, 32, 32);
    font-size: 16px;
    margin-top: 2px;
    margin-left: 0;
    margin-bottom: 2px;
    width: 100%;
    height: 100%;
}

.powerSlider {
    flex-direction: row;
}
